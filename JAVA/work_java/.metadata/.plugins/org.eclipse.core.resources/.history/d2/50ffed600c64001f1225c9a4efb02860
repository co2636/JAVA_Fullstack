
// 예제 : 2단부터 9단까지 구구단 출력하

public class E03 {

	public static void main(String[] args) {
		/*
		 	** 2 단** 
		 	2 * 1 = 2
		 	2 * 2 = 4
		 	....
		 	2* 9 =18
		 	
		 	**3단**
		 */
		int i,j;
		
		for(i=2;i<10;i++) {
			System.out.println("**" + i+"단**");
			for(j=1;j<10;j++) {
		
				System.out.println(i + " * " + j + " = " +(i*j));
		
			}
			System.out.println("---------------------");
		}
	}

}
