
public class test1 {

	public static void main(String[] args) {
		
		// 문제 1.
		int x = 10;
		int y = 20;
		int z = (++x) + (y--);
		/*
		 	z = (++x) + (y--);
		 	z = ++10 + 20--;
		 	z = 11 + 20
		 	z = 31
		 	
		 */
		System.out.println(z); // 31
//		------------------------------------------------------------
		
		// 문제 2.
		int score = 85;
		String result = (!(score > 90))? "가" : "나";
		System.out.println(result);
		
		
	}

}
